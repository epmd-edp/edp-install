[
  {
    "displayName": "Tracing system (Zipkin)",
    "command": {
      "name": "zipkin",
      "image": "openzipkin/zipkin",
      "version": "2.6.0",
      "storage": {
        "class": "{{ storage_class_name }}",
        "capacity": "1Gi"
      }
    }
  },
  {
    "displayName": "Message broker (RabbitMQ)",
    "command": {
      "name": "rabbitmq",
      "image": "rabbitmq",
      "version": "3.7.5-management",
      "storage": {
        "class": "{{ storage_class_name }}",
        "capacity": "1Gi"
      }
    }
  },
  {
    "displayName": "Metrics aggregator (Turbine)",
    "command": {
      "name": "turbine",
      "image": "epamedp/turbine",
      "version": "1.2.0"
    }
  },
  {
    "displayName": "Monitoring dashboard (Hystrix)",
    "command": {
      "name": "hystrix-dashboard",
      "image": "epamedp/hystrix-dashboard",
      "version": "1.2.0"
    }
  },
  {
    "displayName": "Identity service (Keycloak)",
    "command": {
      "name": "keycloak",
      "image": "jboss/keycloak",
